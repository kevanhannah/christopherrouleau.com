{
  "name": "christopherrouleaucom",
  "description": "Content management studio for christopherrouleau.com.",
  "main": "package.json",
  "author": "Kevan Hannah",
  "scripts": {
    "start": "sanity start",
    "build": "sanity build",
    "dev": "sanity dev",
    "deploy-graphql": "sanity graphql deploy"
  },
  "keywords": [
    "sanity"
  ],
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
